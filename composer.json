{
  "name": "sharpapi/php-custom-workflow",
  "description": "PHP SDK for SharpAPI Custom AI Workflow - build and execute no-code AI API endpoints",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "SharpAPI",
    "custom",
    "workflows",
    "ai",
    "api",
    "no-code",
    "php"
  ],
  "homepage": "https://sharpapi.com/",
  "authors": [
    {
      "name": "A2Z WEB LTD",
      "homepage": "https://sharpapi.com/"
    }
  ],
  "require": {
    "php": ">=8.1",
    "sharpapi/php-core": "^1.4.0",
    "guzzlehttp/guzzle": "^7.0|^7.9|^7.10"
  },
  "require-dev": {
    "pestphp/pest": "^2.0|^3.0"
  },
  "autoload": {
    "psr-4": {
      "SharpAPI\\CustomWorkflow\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SharpAPI\\CustomWorkflow\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "source": "https://github.com/sharpapi/php-custom-workflow",
    "issues": "https://github.com/sharpapi/php-custom-workflow/issues"
  }
}
